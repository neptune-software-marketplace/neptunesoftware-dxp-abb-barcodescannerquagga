							"fieldParent": "d34ecefa-6d34-4365-f2bc-a82123dd312c",
							"fieldType": "neptune.Text",
							"script": [
								"==OBJECT STRING==",
								"Add this to your Application + Launchpad header:",
								"",
								"<script src=\"https://cdnjs.cloudflare.com/ajax/libs/quagga/0.12.1/quagga.min.js\"></script>",
								"",
